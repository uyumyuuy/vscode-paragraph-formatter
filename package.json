{
  "name": "vscode-paragraph-formatter",
  "displayName": "Paragraph Formatter",
  "description": "Text formatter  for machine translation.",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.59.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vscode-paragraph-formatter.format"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-paragraph-formatter.format",
        "title": "%title_format%"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "node ./test/runTest.js"
  },
  "publisher": "uyum",
  "author": {
    "name": "uyum"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/uyumyuuy/vscode-vscode-paragraph-formatter.git"
  },
  "homepage": "https://github.com/uyumyuuy/vscode-vscode-paragraph-formatter/blob/main/README.md",
  "bugs": {
    "url": "https://github.com/uyumyuuy/vscode-vscode-paragraph-formatter/issues"
  },
  "devDependencies": {
    "@types/vscode": "^1.59.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  }
}
